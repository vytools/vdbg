{
	"name": "vdbg-debugger",
	"description": "Visual Debugger - An extensible visual debugger",
	"version": "0.3.13",
	"publisher": "vytools",
	"private": true,
	"icon": "media/logo128.png",
	"license": "MIT",
	"files": [
		"dist/extension.js",
		"resources/",
		"media/builtin/",
		"media/styles/",
		"media/vyjs/css/",
		"media/vyjs/js/",
		"media/logo128.png",
		"package.json",
		"node_modules/katex/dist/katex.min.css",
		"LICENSE"
	],	
	"repository": {
		"type": "git",
		"url": "https://github.com/natebu/vdbg"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Debuggers",
		"Visualization"
	],
	"activationEvents": [
		"onDebug",
		"workspaceContains:.vscode/vdbg.json"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vDbg.start",
				"title": "Start vdbg",
				"category": "vDbg"
			}
		]
	},
	"scripts": {
		"esbuild": "node scripts/esbuild.js",
        "esbuild-watch": "node scripts/esbuild.js --watch",
        "esbuild-prod": "node scripts/esbuild.js --production"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"esbuild": "^0.25.11",
		"eslint": "^7.21.0",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"adm-zip": "^0.5.10",
		"highlight.js": "^11.11.1",
		"json5": "^2.2.3",
		"katex": "^0.16.25",
		"micromark": "^4.0.2",
		"micromark-extension-gfm": "^3.0.0",
		"micromark-extension-math": "^3.1.0",
		"ws": "^8.18.2"
	}
}
